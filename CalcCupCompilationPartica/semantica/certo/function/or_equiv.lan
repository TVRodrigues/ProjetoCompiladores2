or a b ::  Bool -> Bool -> Bool {
    return !(!a && !b);
}

printbol x :: Bool -> Void{
    if(x){ print('T');}else{ print('F');}
}

main :: Void{
   a = true;
   b = true;
   printbol(or(a b)[0]);
   print('\n');
   a = true;
   b = false;
   printbol( or(a b)[0]);
   print('\n');
   a = false;
   b = true;
   printbol(or(a b)[0]);
   print('\n');
   a = false;
   b = false;
   printbol(or(a b)[0]);
   print('\n');
}
